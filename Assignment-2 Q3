public class FuelPostfixSum {
    // Function to compute the postfix sum of fuel consumption
    public static int[] computePostfixSum(int[] fuel) {
        int m = fuel.length;
        int[] postfixSum = new int[m]; 
        // Initialize the last element of postfix sum
        if (m > 0) {
            postfixSum[m - 1] = fuel[m - 1];
        }
        // Compute the postfix sums
        for (int i = m - 2; i >= 0; i--) {
            postfixSum[i] = postfixSum[i + 1] + fuel[i];
        }
        return postfixSum;
    }
    // Main method to test the code
    public static void main(String[] args) {
        int[] fuel = {5, 10, 3, 7, 8};
        int[] postfixSum = computePostfixSum(fuel);
        // Print the postfix sum array
        System.out.print("PostfixSum[] = [");
        for (int i = 0; i < postfixSum.length; i++) {
            System.out.print(postfixSum[i]);
            if (i < postfixSum.length - 1) {
                System.out.print(", ");
            }
        }
        System.out.println("]");
    }
}
