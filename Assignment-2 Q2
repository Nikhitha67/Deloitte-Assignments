public class SalesPrefixSum {
    // Function to compute the prefix sum of sales
    public static int[] computePrefixSum(int[] sales) {
        int n = sales.length;
        int[] prefixSum = new int[n];
        // Initialize the first element of prefix sum
        if (n > 0) {
            prefixSum[0] = sales[0];
        }
        // Compute the prefix sums
        for (int i = 1; i < n; i++) {
            prefixSum[i] = prefixSum[i - 1] + sales[i];
        }
        return prefixSum;
    }
    // Main method to test the code
    public static void main(String[] args) {
        int[] sales = {10, 20, 15, 30, 25};
        int[] prefixSum = computePrefixSum(sales);       
        // Print the prefix sum array
        System.out.print("PrefixSum[] = [");
        for (int i = 0; i < prefixSum.length; i++) {
            System.out.print(prefixSum[i]);
            if (i < prefixSum.length - 1) {
                System.out.print(", ");
            }
        }
        System.out.println("]");
    }
}
